$C = "../Common.ui";

$C.@PageOverlay {}

$C.@Container {
  Anchor: (Width: 760, Height: 700);

  #Title {
    Group {
      $C.@Title {
        @Text = "HYPERKS COSMETICS";
      }

      $C.@HeaderSearch {}
    }
  }

  #Content {
    LayoutMode: Left;

    Group #CosmeticList {
      FlexWeight: 1;
      LayoutMode: TopScrolling;
      ScrollbarStyle: $C.@DefaultScrollbarStyle;
    }
  }
}

$C.@BackButton {}
